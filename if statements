#include <stdio.h>
int main(void)
{
    int num1,num2;
    printf("Enter 2 numbers:\n");
    scanf("%d%d",&num1,&num2);
    if(num1==num2) {
    printf("%d is equal to %d",num1,num2);
    } 
    if(num1>num2) {
    printf("%d is greater than %d",num1,num2);    
    }
    if(num1<num2){
    printf("%d is less than %d",num1,num2);
        
    }
    
    
    
    return(0);
 
}
